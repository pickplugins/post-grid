{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "post-grid/menu-wrap-item",
  "title": "Menu Item",
  "textdomain": "post-grid",
  "category": "post-grid",
  "usesContext": [
    "postId",
    "loopIndex",
    "postType",
    "queryId"
  ],
  "supports": {
    "align": [
      "wide",
      "full"
    ]
  },
  "parent": [
    "post-grid/menu-wrap"
  ],
  "attributes": {
    "wrapper": {
      "type": "object",
      "default": {
        "options": {
          "tag": "li",
          "class": "flex-item-wrap",
          "isActive": false
        },
        "styles": {
          "backgroundColor": {
            "Desktop": ""
          }
        }
      }
    },
    "link": {
      "type": "object",
      "default": {
        "options": {
          "class": "",
          "text": "",
          "url": ""
        },
        "styles": {
          "backgroundColor": {
            "Desktop": ""
          }
        }
      }
    },
    "subMenuWrap": {
      "type": "object",
      "default": {
        "options": {
          "class": "",
          "type": ""
        },
        "styles": {
          "backgroundColor": {
            "Desktop": ""
          }
        }
      }
    },
    "icon": {
      "type": "object",
      "default": {
        "options": {
          "enable": true,
          "library": "fontAwesome",
          "srcType": "class",
          "iconSrc": "fas fa-check-circle",
          "position": "beforeText",
          "class": "link-icon"
        },
        "styles": {
          "color": {
            "Desktop": ""
          }
        }
      }
    },
    "blockId": {
      "type": "string",
      "default": ""
    },
    "customCss": {
      "type": "string",
      "default": ""
    },
    "blockCssY": {
      "type": "object",
      "default": {
        "items": {}
      }
    }
  },
  "editorStyle": "pgmenu_wrap_editor_style",
  "keywords": [
    "menu",
    "menu block",
    "navigation block",
    "menu for wordpress",
    "menu block for wordpress"
  ],
  "description": "This block is the inner block of menu wrap block"
}