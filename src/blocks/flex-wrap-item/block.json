{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "post-grid/flex-wrap-item",
  "title": "Flex Item",
  "textdomain": "post-grid",
  "category": "post-grid-tools",
  "parent": [
    "post-grid/flex-wrap"
  ],
  "usesContext": [
    "postId",
    "loopIndex",
    "postType",
    "queryId"
  ],
  "supports": {
    "align": [
      "wide",
      "full"
    ]
  },
  "attributes": {
    "wrapper": {
      "type": "object",
      "default": {
        "options": {
          "tag": "div",
          "class": "pg-flex-wrap-item"
        },
        "styles": {
          "backgroundColor": {
            "Desktop": ""
          },
          "flexBasis": {
            "Desktop": "0"
          },
          "flexGrow": {
            "Desktop": "1"
          }
        }
      }
    },
    "blockId": {
      "type": "string",
      "default": ""
    },
    "blockCssY": {
      "type": "object",
      "default": {
        "items": {}
      }
    }
  },
  "script": "front_script",
  "keywords": [
    "flex wrapper",
    "flex layout",
    "flex wordpress",
    "flex block for wordpress"
  ],
  "description": "This block is the inner block of flex wrap block"
}