{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "post-grid/grid-wrap-item",
  "title": "Grid Item",
  "textdomain": "post-grid",
  "category": "post-grid",
  "parent": ["post-grid/grid-wrap"],
  "usesContext": [
    "postId", "loopIndex", "postType", "queryId"
  ],
  "supports": {
    "align": [
      "wide",
      "full"
    ]
  },
  
  "attributes": {
    "wrapper": {
      "type": "object",
      "default": {
        "options": {
          "tag": "div",
          "class": "grid-item-wrap"
        },
        "styles": {
          "backgroundColor": {
            "Desktop": ""
          }
        }
      }
    },
    "blockId": {
      "type": "string",
      "default": ""
    },
    "customCss": {
      "type": "string",
      "default": ""
    },
    "blockCssY": {
      "type": "object",
      "default": {
        "items": {}
      }
    }
  },
  "keywords": [
    "advanced accordion",
    "nested accordion",
    "customizable accordion"
  ],
  "description": "Shows warning, error or success notices..."
}