{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "post-grid/woo-breadcrumb",
  "title": "Woo breadcrumb",
  "textdomain": "post-grid",
  "category": "post-grid-woo",
  "usesContext": [
    "postId",
    "loopIndex",
    "postType",
    "queryId"
  ],
  "supports": {
    "align": [
      "wide",
      "full"
    ]
  },
  "attributes": {
    "wrapper": {
      "type": "object",
      "default": {
        "options": {
          "tag": "div",
          "class": "pg-woo-breadcrumb"
        },
        "styles": {
          "color": {
            "Desktop": ""
          },
          "backgroundColor": {
            "Desktop": ""
          },
          "padding": {
            "Desktop": ""
          },
          "margin": {
            "Desktop": ""
          }
        }
      }
    },
    "visible": {
      "type": "object",
      "default": {}
    },
    "blockId": {
      "type": "string",
      "default": ""
    },
    "blockCssY": {
      "type": "object",
      "default": {
        "items": {}
      }
    }
  },
  "style": "front_style",
  "script": "front_script",
  "keywords": [
    "Woo breadcrumb",
    "woocommerce breadcrumb",
    "woocommerce breadcrumbs"
  ],
  "description": "woocommerce breadcrumbs block ."
}