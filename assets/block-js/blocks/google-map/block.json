{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "post-grid/google-map",
  "title": "Google Map",
  "textdomain": "post-grid",
  "category": "post-grid-tools",
  "usesContext": [
    "postId",
    "loopIndex",
    "postType",
    "queryId"
  ],
  "supports": {
    "align": [
      "wide",
      "full"
    ]
  },
  "attributes": {
    "wrapper": {
      "type": "object",
      "default": {
        "options": {
          "tag": "div",
          "class": "pg-google-map",
          "id": "",
          "linkTo": "postUrl",
          "linkToAuthorMeta": "",
          "linkToCustomMeta": "",
          "linkTarget": "_blank",
          "customUrl": ""
        },
        "styles": {
          "height": {
            "Desktop": "400px"
          }
        }
      }
    },
    "addresses": {
      "type": "array",
      "default": []
    },
    "objMap": {
      "type": "object",
      "default": {
        "zoom": "10",
        "zoomControl": "true",
        "minZoom": "1",
        "maxZoom": "16",
        "lat": "-33.860664",
        "lng": "151.208138",
        "tilt": "",
        "heading": "",
        "name": "",
        "titleSize": "",
        "alt": "",
        "mapTypeId": "roadmap",
        "streetViewControl": "true",
        "colorScheme": "DARK"
      }
    },
    "visible": {
      "type": "object",
      "default": {}
    },
    "blockId": {
      "type": "string",
      "default": ""
    },
    "blockCssY": {
      "type": "object",
      "default": {
        "items": {}
      }
    }
  },
  "script": "front_script",
  "keywords": [
    "layer",
    "html element",
    "div element",
    "div",
    "html tag",
    "layer block",
    "layer for wordpress",
    "layer block for wordpress"
  ],
  "description": "The layer block is an HTML element that can be styled and positioned on a webpage to structure and organize content."
}